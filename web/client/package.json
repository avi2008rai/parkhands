    "client-dev": "cd ../; npm run client-dev",
    "get-static-files-local": "time wget -O ./data/processed/dataset.slots.json $CDN_URL/static-slots/$DATASET_CURRENT_FILENAME && time wget -O ./data/processed/dataset.spaces.json $CDN_URL/static-spaces/$DATASET_CURRENT_FILENAME",
    "get-static-files-remote": "echo \"Downloading from: $CDN_URL File: $DATASET_CURRENT_FILENAME\" && wget -O /web/client/data/processed/dataset.slots.json $CDN_URL/static-slots/$DATASET_CURRENT_FILENAME && wget -O /web/client/data/processed/dataset.spaces.json $CDN_URL/static-spaces/$DATASET_CURRENT_FILENAME"
